{
  "name": "svelte-electron-template",
  "version": "0.0.1",
  "description": "The bare minimum boilerplate to use svelte in electron",
  "main": "main.js",
  "scripts": {
    "start": "run-p start:electron build:watch",
    "start:electron": "electron .",
    "build": "rollup -c",
    "build:watch": "rollup -c -w"
  },
  "repository": "https://github.com/chuanqisun/svelte-electron-template",
  "keywords": [
    "svelte",
    "electron",
    "boilerplate",
    "template"
  ],
  "author": "Chuanqi Sun",
  "license": "MIT",
  "devDependencies": {
    "fa-svelte": "^3.1.0",
    "fontmetrics": "^1.0.0",
    "npm-run-all": "^4.1.5",
    "pug": "^3.0.0",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-node-resolve": "^5.0.4",
    "rollup-plugin-sass": "^1.2.2",
    "rollup-plugin-svelte": "^5.1.0",
    "rollup-plugin-terser": "^6.1.0",
    "sass": "^1.26.9",
    "svelte": "^3.5.3",
    "svelte-preprocess": "^3.9.10",
    "webfontloader": "^1.6.28"
  },
  "dependencies": {
    "@fortawesome/free-regular-svg-icons": "^5.13.1",
    "@fortawesome/free-solid-svg-icons": "^5.13.1",
    "electron": "^11.5.0",
    "install": "^0.13.0",
    "npm": "^6.14.5",
    "png-js": "^1.0.0",
    "rollup": "^2.18.2"
  }
