{
  "name": "svelte-electron-template",
  "version": "0.0.1",
  "description": "The bare minimum boilerplate to use svelte in electron",
  "main": "main.js",
  "scripts": {
    "start": "run-p start:electron build:watch",
    "start:electron": "electron .",
    "build": "rollup -c",
    "build:watch": "rollup -c -w"
  },
  "repository": "https://github.com/chuanqisun/svelte-electron-template",
  "keywords": [
    "svelte",
    "electron",
    "boilerplate",
    "template"
  ],
  "author": "Chuanqi Sun",
  "license": "MIT",
  "devDependencies": {
    "electron": "^5.0.5",
    "npm-run-all": "^4.1.5",
    "pug": "^3.0.0",
    "rollup": "^1.16.2",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-node-resolve": "^5.0.4",
    "rollup-plugin-sass": "^1.2.2",
    "rollup-plugin-svelte": "^5.1.0",
    "rollup-plugin-terser": "^6.1.0",
    "sass": "^1.26.9",
    "svelte": "^3.5.3",
    "svelte-preprocess": "^3.9.10"
  }
}
